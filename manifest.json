  {
    "manifest_version": 3,
    "name": "Cookie Logger",
    "version": "1.0",
    "description": "Logs cookies of the current website to the console",
    "permissions": ["cookies", "activeTab", "scripting", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Get Cookies"
    }
  }