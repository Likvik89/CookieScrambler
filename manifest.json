  {
    "manifest_version": 3,
    "name": "Cookie Logger",
    "version": "0.1.9",
    "description": "Scrambles the value of cookies on websites. (Often it does it sometimes)",
    "action": {
      "default_popup": "popup.html",
      "default_title": "Get Cookies",
      "default_icon": "cookei.png"
    },
    "permissions": ["cookies", "activeTab", "scripting", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    }
  }